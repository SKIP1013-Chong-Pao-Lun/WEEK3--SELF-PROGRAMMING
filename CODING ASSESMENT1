package numericData;

public class Exercise1 {
/*
 * Add five integer and display their sum. 
 * Display average for five integer
 */
	// Save as "Exercise1.java"
	public static void main(String[] args) {//Dalam class mesti ada main method
		// TODO Auto-generated method stub
		int number1 =11;//Declare 5 integer variables and assign a value
		int number2=22;
		int number3=33;
		int number4=44;
		int number5=55;
		int totalnumber=5;
		double number6=66;
		double number7=77;
		float number8=88;
		float number9=99;
		double multiplication;
		int sum;//Declare an integer variable called sum to hold the sum
		int average;//Declare an integer variable called sum to hold the average
		double divide;
		float divide1;
		float play;
		
		sum= number1+ number2+number3+number4+number5;//Compute the sum
		average= sum/totalnumber;//Compute the average
		divide=number7/number6;
		divide1=number9/number8;
		multiplication= number1*number2*number3*number4*number5*number6*number7*number8*number9;
		play= (number1*number2)/number3;
		System.out.print("The sum is ");//Print a descriptive string
										//Cursor stays after the printed string
		System.out.println(sum);//print the value stored in variable sum
								//Cursor advances to the beginning of the next line
		System.out.print("The average is ");//Print a descriptive string
		System.out.println(average);
		System.out.printf("The divider for number5 with number2 is :%.2f ",divide);//Print a descriptive string
		System.out.printf("\nThe divider for number9 with number8 is %.2f" ,divide1);//Print a descriptive string
		System.out.println("the multiplication is"+multiplication);
		System.out.printf("The ans for play is :%.2f ",play);//Print a descriptive string

	}

}
